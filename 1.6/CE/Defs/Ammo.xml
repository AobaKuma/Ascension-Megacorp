<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<CombatExtended.AmmoCategoryDef>
		<defName>USAC_DU</defName>
		<label>Depleted uranium</label>
		<labelShort>DU</labelShort>
		<description>Uses depleted uranium as projectile core, It has chemical toxicity and radioactivity., greatly improving armor penetration.</description>
		<advanced>true</advanced>
	</CombatExtended.AmmoCategoryDef>

	<!-- .277 -->
	<ThingDef Class="CombatExtended.AmmoDef" ParentName="277FuryBase">
		<defName>USAC_Ammo_277Fury_DU</defName>
		<label>.277 Fury (DU)</label>
		<graphicData>
			<texPath>Things/Ammo/Rifle/Battlerifle/FMJ</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
		<ammoClass>USAC_DU</ammoClass>
		<statBases>
			<MarketValue>0.25</MarketValue>
		</statBases>
		<tradeTags>
			<li>USAC_Ammunition</li>
		</tradeTags>
		<cookOffProjectile>USAC_Bullet_277Fury_DU</cookOffProjectile>
	</ThingDef>

	<ThingDef ParentName="Base277FuryBullet">
		<defName>USAC_Bullet_277Fury_DU</defName>
		<label>.277 Fury bullet (DU)</label>
		<projectile Class="CombatExtended.ProjectilePropertiesCE">
			<damageAmountBase>14</damageAmountBase>
			<armorPenetrationSharp>22</armorPenetrationSharp>
			<armorPenetrationBlunt>90.500</armorPenetrationBlunt>
			<secondaryDamage>
				<li>
					<def>Flame_Secondary</def>
					<amount>4</amount>
				</li>
				<li>
					<def>UASC_Radiotoxicity</def>
					<amount>8</amount>
				</li>
			</secondaryDamage>
		</projectile>
	</ThingDef>

	<RecipeDef ParentName="AmmoRecipeBase" MayRequire="aoba.fortress.industrial.nucleardawn">
		<defName>MakeUSAC_Ammo_277Fury_DU</defName>
		<label>make .277 Fury (DU) cartridge x500</label>
		<description>Craft 500 .277 Fury (DU) cartridges.</description>
		<jobString>Making .277 Fury (DU) cartridges.</jobString>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Steel</li>
					</thingDefs>
				</filter>
				<count>26</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>FT_DepletedUranium</li>
					</thingDefs>
				</filter>
				<count>4</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>FT_DepletedUranium</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<USAC_Ammo_277Fury_DU>500</USAC_Ammo_277Fury_DU>
		</products>
		<workAmount>4200</workAmount>
	</RecipeDef>
	
